pipeline {
    agent any
	stages {
	    stage('Initialize') {
    	   steps {
   	        echo 'Initializing the code pipeline'
   	    	}
    	}
    	stage('Build') {
	       steps {
       	    echo 'Starting build .....' 	    
       		}
	    }
	    stage('Deploy') {
    	   steps {
   	        echo 'Deploying the artifact'
   	    	}

    	}

	}
}
